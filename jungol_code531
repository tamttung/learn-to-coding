//jungol_code531

#include<stdio.h>

int main()
{
	double a,b;
	
	scanf("%lf",&a);
	
	if(a<=50.80)
	{
		printf("Flyweight");
	}
	else if (a<=61.23)
	{
		printf("Lightweight");
	}
		else if (a<=72.57)
	{
		printf("Middleweight");
	}	
	else if (a<=88.45)
	{
		printf("Cruiserweight");
	}	
	else 
	{
		printf("Heavyweight");
	}
	
	return 0;
}
